#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/@botpress+cli@0.6.16/node_modules/@botpress/cli/node_modules:/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/@botpress+cli@0.6.16/node_modules/@botpress/node_modules:/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/@botpress+cli@0.6.16/node_modules:/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/@botpress+cli@0.6.16/node_modules/@botpress/cli/node_modules:/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/@botpress+cli@0.6.16/node_modules/@botpress/node_modules:/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/@botpress+cli@0.6.16/node_modules:/Users/aschung/dev/botpress-weavel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@botpress/cli/bin.js" "$@"
else
  exec node  "$basedir/../@botpress/cli/bin.js" "$@"
fi
